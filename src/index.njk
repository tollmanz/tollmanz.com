<div class="home">
  <ul class="post-list">
    {% for post in collections.posts %}
      <li>
        <span class="post-meta">{{ post.date | dateDisplay }}</span>
        <h2>
          <a class="post-link" href="{{ post.url }}">{{ post.data.title }}</a>
        </h2>
      </li>
    {% endfor %}
  </ul>
  <p class="rss-subscribe">subscribe <a href="/feed.xml">via RSS</a></p>
</div>
